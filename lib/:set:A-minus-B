# $Id: :set:A-minus-B,v 1.1 2016/05/11 18:04:46 tw Exp $
# vim: filetype=zsh tabstop=4 textwidth=72 noexpandtab

emulate -L zsh

setopt unset
typeset -a only_A=()
for a in ${(P)1}; do
	[[ -z ${(P)2[(r)$a]} ]] && only_A+=( $a )
done

print -N $only_A

