# vim: filetype=zsh tabstop=4 textwidth=72 noexpandtab
# play some music files using amuse

emulate -L zsh
setopt local_options local_traps pipe_fail bsd_echo

typeset -- amuse=$me/util/amuse/amuse-master.zsh
needs $amuse

-:music-in-fs $@
$amuse $reply

